{
  "name": "sbk-schedule",
  "version": 1,
  "type": "docker",
  "alias": "schedule.somervillebikekitchen.org",
  "public": true,
  "scale": {
    "sfo": {
      "min": 1,
      "max": 1
    }
  },
  "env": {
    "NODE_ENV": "production",
    "IAM_EMAIL": "@iam_email",
    "IAM_PRIVATE_KEY_BASE64": "@iam_private_key",
    "MAILGUN_API": "@mailgun_api_key",
    "PORT": "3030"
  }
}
