{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "google",
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "newline-per-chained-call": ["error", {"ignoreChainWithDepth": 2}],
    "require-jsdoc": "off",
    "max-len": ["warn", {"code": 120}]
  },
  "env": {
    "es6": true
  }
}
