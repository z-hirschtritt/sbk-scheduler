{
  "title": "term",
  "type": "object",
  "properties": {
    "id": {
      "type": "number",
      "title": "ID",
      "default": 0,
      "examples": [
        12
      ]
    },
    "start": {
      "type": "string",
      "title": "Start Date of Term",
      "default": "",
      "examples": [
        "2018-02-04"
      ],
      "pattern": "^([0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])$"
    },
    "end": {
      "type": "string",
      "title": "End Date of Term",
      "default": "",
      "examples": [
        "2018-05-04"
      ],
      "pattern": "^([0-9]{4})-(1[0-2]|0[1-9])-(3[01]|0[1-9]|[12][0-9])$"
    }
  },
  "required": [
    "id",
    "start",
    "end"
  ]
}